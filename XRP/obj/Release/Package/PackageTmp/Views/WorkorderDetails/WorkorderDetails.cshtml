@model XRP.Models.ssmtbl_WorkorderDetailsModelNew
@{
    ViewBag.Title = "Workorder Details";
    ViewBag.Active = "Marketing";
    // Layout = "~/Views/Shared/_NewLayout.cshtml";
    Layout = null;
}


<fieldset>

    <div class="page-title">
        <h2><span class="fa fa-arrow-circle-o-left"></span>Workorder Details</h2>
    </div>
    <div class="row">
        <div class="container">
            <table class="table table-bordered table-condensed table-responsive">
                <tr>
                    <td>
                        @Html.LabelFor(model => model.WorkorderNo, new { @class = "Label" })
                    </td>
                    <td>
                        @Html.TextBoxFor(model => model.WorkorderNo, null, new { @class = "form-control NoStar", @readonly = "readonly", @style = "border: 0px; color:Black" })
                    </td>
                    <td>@Html.LabelFor(model => model.RMQty, new { @class = "Label" })</td>
                    <td>@Html.TextBoxFor(model => model.RMQty, null, new { @class = "form-control NoStar", @readonly = "readonly", @style = "border: 0px; color:Black" })</td>

