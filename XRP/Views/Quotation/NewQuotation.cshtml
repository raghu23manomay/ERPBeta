@model XRP.Models.ssmtbl_QuotationModel
@using XRP.Models;
@{
    ViewBag.Title = "Quotation";
    ViewBag.Active = "Marketing";
    Layout = "~/Views/Shared/_DesignLayout.cshtml";

}

<link href="~/Content/Validations/jquery-ui.css" rel="stylesheet" />
<style>
    /*span, small {
        color: black;
    }*/
    ::-webkit-input-placeholder {
        font-style: italic;
        font-weight: bold;
    }

    :-moz-placeholder {
        font-style: italic;
        font-weight: bold;
    }

    ::-moz-placeholder {
        font-style: italic;
        font-weight: bold;
    }

    :-ms-input-placeholder {
        font-style: italic;
        font-weight: bold;
    }
</style>
@using (Html.BeginForm("CreateQuotation", "Quotation", FormMethod.Post, new { @class = "form-horizontal", role = "form", enctype = "multipart/form-data" }))
{@Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>

        <header class="blue accent-3 relative">
            <div class="container-fluid text-white">
                @*<div class="row p-t-b-10 ">
                    <div class="col">*@
                <h4>
                    <i class="icon-database"></i>
                    Quotation
                </h4>
                @*</div>
                    </div>*@
                <div class="row justify-content-between">
                    <ul class="nav nav-material nav-material-white responsive-tab" id="v-pills-tab" role="tablist">
                        <li>
                            <a class="nav-link" href='@Url.Action("AllInquiry", "Inquiry")'><i class="icon icon-home2"></i>Enquiry List</a>

                        </li>

                        <li class="float-right">
                            <a class="nav-link active"><i class="icon icon-plus-circle"></i>Quotation</a>
                        </li>
                    </ul>
                </div>
            </div>
            <div class="col-md-12">
                <div class="@TempData["HideClass"]" style="text-align:center; display:none">
                    @TempData["Message"]
                </div>
            </div>
            @Html.HiddenFor(m => m.QuotationId, new { @id = "hdn_QuotationId" })
            @Html.HiddenFor(m => m.InquiryNo, new { @id = "hdn_InquiryNo" })
            @Html.HiddenFor(m => m.CustomerName, new { @id = "hdn_CustomerName" })
            @Html.HiddenFor(m => m.QuotationNo, new { @id = "hdn_QuotationNo" })
            @Html.HiddenFor(m => m.CuttingRate, new { @id = "hdn_CuttingRate" })
            @Html.HiddenFor(m => m.PerPieceRate, new { @id = "hdn_PerPieceRate" })
            @Html.HiddenFor(model => model.SectionId, new { @id = "hdn_SectionId" })
            @Html.HiddenFor(model => model.Section, new { @id = "hdn_Section" })
            @Html.HiddenFor(model => model.FileStatus, new { @id = "hdn_FileStatus" })
            @Html.HiddenFor(model => model.PartDescription, new { @id = "hdn_PartDescription" })
            @Html.HiddenFor(model => model.PartNo, new { @id = "hdn_PartNo" })
            @Html.HiddenFor(model => model.HeatTreatmentText, new { @id = "hdn_HeatTreatmentText" })
            @Html.HiddenFor(model => model.HeatTreatmentID, new { @id = "hdn_HeatTreatmentID" })
            @Html.HiddenFor(model => model.Material, new { @id = "hdn_Material" })
            @Html.HiddenFor(model => model.MachainedWeight, new { @id = "hdn_MachainedWeight" })
            @Html.HiddenFor(model => model.ForgingCostBased, new { @id = "hdn_ForgingCostBased" })
            @Html.HiddenFor(model => model.MPICostBasedOn, new { @id = "hdn_MPICostBasedOn" })
            @Html.HiddenFor(model => model.Total, new { @id = "hdn_Total" })
            @Html.HiddenFor(model => model.CuttingSquare1, new { @id = "hdn_CuttingSquare1" })
            @Html.HiddenFor(model => model.CuttingSquare2, new { @id = "hdn_CuttingSquare2" })
            @Html.HiddenFor(model => model.Forging1, new { @id = "hdn_Forging1" })
            @Html.HiddenFor(model => model.Forging2, new { @id = "hdn_Forging2" })
            @Html.HiddenFor(model => model.HeatTreatment1, new { @id = "hdn_HeatTreatment1" })
            @Html.HiddenFor(model => model.HeatTreatment2, new { @id = "hdn_HeatTreatment2" })
            @Html.HiddenFor(model => model.ShotBlasting1, new { @id = "hdn_ShotBlasting1" })
            @Html.HiddenFor(model => model.ShotBlasting2, new { @id = "hdn_ShotBlasting2" })
            @Html.HiddenFor(model => model.MPI1, new { @id = "hdn_MPI1" })
            @Html.HiddenFor(model => model.MPI2, new { @id = "hdn_MPI2" })
            @Html.HiddenFor(model => model.Grinding1, new { @id = "hdn_Grinding1" })
            @Html.HiddenFor(model => model.Grinding2, new { @id = "hdn_Grinding2" })
            @Html.HiddenFor(model => model.ColdCoining1, new { @id = "hdn_ColdCoining1" })
            @Html.HiddenFor(model => model.ColdCoining2, new { @id = "hdn_ColdCoining2" })
            @Html.HiddenFor(model => model.Machining1, new { @id = "hdn_Machining1" })
            @Html.HiddenFor(model => model.Machining2, new { @id = "hdn_Machining2" })
            @Html.HiddenFor(model => model.DieMaintenance1, new { @id = "hdn_DieMaintenance1" })
            @Html.HiddenFor(model => model.DieMaintenance2, new { @id = "hdn_DieMaintenance2" })
            @Html.HiddenFor(model => model.chk_NewQuotation)
            @Html.HiddenFor(model => model.InitialDieCost, new { @id = "hdn_initialdiecost" })
            <input type="hidden" id="hdnForgeConversionCost" />
        </header>

        <!-- PAGE TITLE -->
        <div class="container-fluid animatedParent animateOnce">
            <div class="animated fadeInUpShort">
                <div class="row my-3">
                    <div class="col-md-12">
                        <form action="#">
                            <div class="card no-b  no-r">
                                @*<div class="card-body">*@
                                <div class="row row-eq-height">
                                    <!-- Daily Sale Report-->
                                    <div class="col-md-12">
                                        <div class="card my-3 no-b" style="padding-bottom:0px;">
                                            <div class="card-header white b-0 p-3">
                                                <div class="card-handle">
                                                    <small class="card-subtitle mb-2 text-muted">Version : @Html.DisplayFor(m => m.new_Version_No) |</small>
                                                    <span class="badge r-3 badge-success "> @Html.DisplayFor(m => m.FileStatus)</span>
                                                </div>
                                                <h4 class="card-title">@Html.DisplayFor(m => m.QuotationNo) &nbsp;|&nbsp;@Html.DisplayFor(m => m.CustomerName)</h4>
                                                <span>
                                                    Part # <label class="Label">@Html.DisplayFor(m => m.PartNo)</label>
                                                    | Description : <label class="Label">@Html.DisplayFor(m => m.PartDescription)</label>
                                                    | Material : <label class="Label"> @Html.DisplayTextFor(m => m.Material) </label>
                                                    | Section : <label class="Label"> @Html.DisplayTextFor(m => m.Section) </label>

                                                </span>
                                                <br />
                                                <span>
                                                    Heat Treatment : <label class="Label">@Html.DisplayTextFor(m => m.HeatTreatmentText)</label>
                                                </span>
                                               
                                             
                                                    <span>
                                                        | Recommended Mill:
                                                        @foreach (var item in ViewData["MultiVendors"] as List<ssmtbl_Feasibility_MultiVendorModel>)                                                     {
                                                            <label class="Label">@Html.Label(item.VendorName),</label>

                                                        }
                                                    </span>
                                               



                                                  
                                                    <p></p>
                                                    <div class="custom-control-inline">
                                                        @*<strong> <u>Raw Material Cost Details:-</u> </strong>*@
                                                        @if (ViewData["NatureofWork"].ToString() == "201")
                                                        {
                                                            <span>Steel Rate Per Kg:&nbsp; &nbsp;  </span>
                                                                @Html.TextBoxFor(model => model.SteelCost, new { @class = "form-control r-0 light s-12 form-control-sm", @readonly = "readonly", @style = "width:70px;background:#F5F8FA;", @oninput = "return CalcRawMaterialTotal();" })
                                                        }
                                                        else
                                                        {
                                                            <span>Steel Rate Per Kg:&nbsp; &nbsp;  </span>
                                                                @Html.TextBoxFor(model => model.SteelCost, new { @class = "form-control r-0 light s-12 form-control-sm", @style = "width:70px;background:#F5F8FA;", @oninput = "return CalcRawMaterialTotal();" })
                                                        }
                                                        &nbsp; &nbsp; <span>Transportation:  </span>&nbsp;&nbsp;  @Html.TextBoxFor(model => model.Transportation, new { @class = "form-control r-0 light s-12 form-control-sm", @oninput = "return CalcRawMaterialTotal();", @style = "width:70px;background:#F5F8FA;" })
                                                        &nbsp; &nbsp; <span>Total RawMaterial Cost: </span>&nbsp;&nbsp;  @Html.TextBoxFor(model => model.TotalRowMaterialCost, new { @class = "form-control r-0 light s-12 form-control-sm", @readonly = "readonly", @style = "width:70px;" })
                                                    </div>
                                                </div>
                                            </div>

                                                <div class="form-row col-md-12" style="padding-top:0px;">
                                                    <div class="col-md-4 mb-3">
                                                        @Html.LabelFor(model => model.ForgingCostBased, new { @class = "Label" })
                                                        <br>

                                                        <div class="custom-control custom-radio custom-control-inline">
                                                            <input type="radio" id="GrossWt" name="ForgingCostBased" class="custom-control-input" value="Gross Wt" onclick="CalcForging2();">
                                                            <label class="custom-control-label m-0" for="GrossWt">Gross Wt</label>
                                                        </div>
                                                        <div class="custom-control custom-radio custom-control-inline">
                                                            <input type="radio" id="CutWt" name="ForgingCostBased" class="custom-control-input" value="Cut Wt" onclick="CalcForging2();">
                                                            <label class="custom-control-label m-0" for="CutWt">Cut Wt</label>
                                                        </div>
                                                        <div class="custom-control custom-radio custom-control-inline">
                                                            <input type="radio" id="NetWt" name="ForgingCostBased" class="custom-control-input" value="Net Wt" onclick="CalcForging2();">
                                                            <label class="custom-control-label m-0" for="NetWt">Net Wt</label>
                                                        </div>


                                                    </div>
                                                    <div class="col-md-4 mb-3">

                                                        <div class="col-md-4" style="display:none" id="divGrossWt">

                                                            <label class="Label">Gross Wt</label><br />
                                                            @Html.HiddenFor(m => m.GrossWeight)
                                                            @Html.DisplayFor(m => m.GrossWeight, new { @class = "form-control r-0 light s-12", @disabled = "disabled" })
                                                        </div>
                                                        <div class="col-md-4" style="display:none" id="divCutWt">
                                                            <label class="Label">Cut Wt</label><br />
                                                            @Html.HiddenFor(m => m.CutWeight)
                                                            @Html.DisplayFor(m => m.CutWeight, new { @class = "form-control r-0 light s-12", @disabled = "disabled" })
                                                        </div>
                                                        <div class="col-md-4" style="display:none" id="divNetWt">
                                                            <label class="Label">Net Wt</label><br />
                                                            @Html.HiddenFor(m => m.NetWeight)
                                                            @Html.DisplayFor(m => m.NetWeight, new { @class = "form-control r-0 light s-12", @disabled = "disabled" })
                                                        </div>


                                                    </div>
                                                    <div class="col-md-4 mb-3">
                                                        @Html.LabelFor(m => m.MPICostBasedOn, new { @class = "Label" })<br />
                                                        <div class="custom-control custom-radio custom-control-inline">
                                                            <input type="radio" id="NetWtMPI" name="MPICostBasedOn" class="custom-control-input" value="Net Wt" onclick="CalcMPI2();">
                                                            <label class="custom-control-label m-0" for="NetWtMPI">Net Wt</label>
                                                        </div>
                                                        <div class="custom-control custom-radio custom-control-inline">
                                                            <input type="radio" id="PerPiece" name="MPICostBasedOn" class="custom-control-input" value="Per Piece" onclick="CalcMPI2();">
                                                            <label class="custom-control-label m-0" for="PerPiece">Per Piece</label>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-row col-md-12">
                                                    @foreach (var item in ViewData["OperationsInvolved"] as List<ssmtbl_Feasibility_OperationsInvolvedModel>)
                                                    {
                                                        if (item.Operations_Involved == 1 && item.Operations_Involved_Select)
                                                        {
                                                            <div class="form-group col-3 m-0">
                                                                <div>@Html.Label("Cutting", new { @class = "label" })</div>
                                                                <div class="custom-control-inline">

                                                                    @Html.TextBoxFor(model => model.CuttingSquare1, new { @class = "form-control r-0 light s-12", @oninput = "return CalcForCuttingSq();", @placeholder = "Rs Per Sq Inch" })
                                                                    &nbsp;&nbsp;
                                                                    @Html.TextBoxFor(model => model.CuttingSquare2, new { @class = "form-control r-0 light s-12", @readonly = "readonly", @placeholder = "Amount" })
                                                                </div>
                                                            </div>
                                                        }
                                                        if (item.Operations_Involved == 2 && item.Operations_Involved_Select)
                                                        {
                                                            <div class="form-group col-3 m-0">
                                                                <div>  @Html.Label("Forging", new { @class = "label" })</div>
                                                                <div class="custom-control-inline">

                                                                    @Html.TextBoxFor(model => model.Forging1, new { @class = "form-control r-0 light s-12", oninput = "CalcForging2()" })
                                                                    &nbsp;&nbsp;
                                                                    @Html.TextBoxFor(model => model.Forging2, new { @class = "form-control r-0 light s-12", @readonly = "readonly", @placeholder = "Amount" })
                                                                </div>
                                                            </div>
                                                        }
                                                        if (item.Operations_Involved == 3 && item.Operations_Involved_Select)
                                                        {
                                                            <div class="form-group col-3 m-0">
                                                                <div><label>Heat Treatment Cost</label></div>
                                                                <div class="custom-control-inline">

                                                                    @Html.TextBoxFor(model => model.HeatTreatment1, new { @class = "form-control r-0 light s-12", @oninput = "return CalcForNetWeight(this);", @placeholder = "Per KG Of Net Wt" })
                                                                    &nbsp;&nbsp;
                                                                    @Html.TextBoxFor(model => model.HeatTreatment2, new { @class = "form-control r-0 light s-12", @readonly = "readonly", @placeholder = "Amount" })
                                                                </div>
                                                            </div>
                                                        }
                                                        if (item.Operations_Involved == 4 && item.Operations_Involved_Select)
                                                        {
                                                            <div class="form-group col-3 m-0">
                                                                <div><label>Shot Blasting</label></div>
                                                                <div class="custom-control-inline">

                                                                    @Html.TextBoxFor(model => model.ShotBlasting1, new { @class = "form-control r-0 light s-12", @oninput = "return CalcForNetWeight(this);", @placeholder = "Net Wt" })
                                                                    &nbsp;&nbsp;

                                                                    @Html.TextBoxFor(model => model.ShotBlasting2, new { @class = "form-control r-0 light s-12", @readonly = "readonly", @placeholder = "Amount" })
                                                                </div>
                                                            </div>
                                                        }
                                                        if (item.Operations_Involved == 5 && item.Operations_Involved_Select)
                                                        {
                                                            <div class="form-group col-3 m-0">
                                                                <div><label>MPI</label></div>
                                                                <div class="custom-control-inline">

                                                                    @Html.TextBoxFor(model => model.MPI1, new { @class = "form-control r-0 light s-12", oninput = "CalcMPI2()", @placeholder = "" })
                                                                    &nbsp;&nbsp;
                                                                    @Html.TextBoxFor(model => model.MPI2, new { @class = "form-control r-0 light s-12", @readonly = "readonly", @placeholder = "Amount" })
                                                                </div>
                                                            </div>
                                                        }
                                                        if (item.Operations_Involved == 13 && item.Operations_Involved_Select)
                                                        {
                                                            <div class="form-group col-3 m-0">
                                                                <div><label>Grinding</label></div>
                                                                <div class="custom-control-inline">

                                                                    @Html.TextBoxFor(model => model.Grinding1, new { @class = "form-control r-0 light s-12", oninput = "CalcForPerPiece(this)", @placeholder = "Per Piece" })
                                                                    &nbsp;&nbsp;
                                                                    @Html.TextBoxFor(model => model.Grinding2, new { @class = "form-control r-0 light s-12", @readonly = "readonly", @placeholder = "Amount" })
                                                                </div>
                                                            </div>
                                                        }
                                                        if (item.Operations_Involved == 14 && item.Operations_Involved_Select)
                                                        {
                                                            <div class="form-group col-3 m-0">
                                                                <div><label>Cold Coining</label></div>
                                                                <div class="custom-control-inline">

                                                                    @Html.TextBoxFor(model => model.ColdCoining1, new { @class = "form-control r-0 light s-12", oninput = "CalcForPerPiece(this)", @placeholder = "Per Piece" })
                                                                    &nbsp;&nbsp;
                                                                    @Html.TextBoxFor(model => model.ColdCoining2, new { @class = "form-control r-0 light s-12", @readonly = "readonly", @placeholder = "Amount" })
                                                                </div>
                                                            </div>
                                                        }
                                                        if (item.Operations_Involved == 15 && item.Operations_Involved_Select)
                                                        {
                                                            <div class="form-group col-3 m-0">
                                                                <div><label>Machaining</label></div>
                                                                <div class="custom-control-inline">

                                                                    @Html.TextBoxFor(model => model.Machining1, new { @class = "form-control r-0 light s-12", oninput = "CalcForPerPiece(this)", @placeholder = "Per Piece" })
                                                                    &nbsp;&nbsp;
                                                                    @Html.TextBoxFor(model => model.Machining2, new { @class = "form-control r-0 light s-12", oninput = "CalcForPerPiece(this)", @readonly = "readonly", @placeholder = "Amount" })
                                                                </div>
                                                            </div>
                                                        }
                                                    }
                                                    <div class="form-group col-3 m-0">
                                                        <div><label>Die Maintaince Cost    </label></div>
                                                        <div class="custom-control-inline">

                                                            @Html.TextBoxFor(model => model.DieMaintenance1, new { @class = "form-control r-0 light s-12", oninput = "CalcForPerPiece(this)", @placeholder = "Per Piece" })
                                                            &nbsp;&nbsp;
                                                            @Html.TextBoxFor(model => model.DieMaintenance2, new { @class = "form-control r-0 light s-12", @readonly = "readonly", @placeholder = "Amount" })
                                                        </div>
                                                    </div>
                                                    <div class="form-group col-3 m-0">
                                                        <div>
                                                            <label>Total Conversion Cost</label> &nbsp;
                                                            <label>Initial Die Cost    </label>
                                                        </div>
                                                        <div class="custom-control-inline">

                                                            @Html.TextBoxFor(model => model.TotalConverstionCost, new { @class = "form-control r-0 light s-12", @readonly = "readonly" })
                                                            &nbsp;&nbsp;
                                                            @Html.TextBoxFor(model => model.InitialDieCost, new { @class = "form-control r-0 light s-12", @onblur = "setDeafult();" })

                                                        </div>


                                                    </div>
                                                    <div class="form-group col-3 m-0">
                                                        <div>
                                                            <label>ICC & Overhead on RM</label> &nbsp;

                                                        </div>
                                                        <div class="custom-control-inline">

                                                            @Html.TextBoxFor(model => model.iccoverhadrm, new { @class = "form-control r-0 light s-12", @id = "txt_iccoverhadrm", @oninput = "caliccoverhead();", @placeholder = "%" })
                                                            &nbsp;&nbsp;
                                                            @Html.TextBoxFor(model => model.totaliccoverhadrm, new { @class = "form-control r-0 light s-12", @onblur = "setDeafult();", @readonly = "readonly", @id = "txt_totaliccoverhadrm", @placeholder = "Amount" })
                                                        </div>

                                                    </div>

                                                    <div class="form-group col-3 m-0">
                                                        <div>
                                                            <label>Profit on Conversion</label> &nbsp;

                                                        </div>
                                                        <div class="custom-control-inline">

                                                            @Html.TextBoxFor(model => model.profitonconversion, new { @class = "form-control r-0 light s-12", @id = "txt_profitconv", @oninput = "calprofitconv();", @placeholder = "%" })
                                                            &nbsp;&nbsp;
                                                            @Html.TextBoxFor(model => model.totalprofitonconversion, new { @class = "form-control r-0 light s-12", @onblur = "setDeafult();", @readonly = "readonly", @id = "txt_totalprofitconv", @placeholder = "Amount" })
                                                        </div>

                                                    </div>
                                                    <div class="form-group col-3 m-0">
                                                        <div>
                                                            <label>Rejection</label> &nbsp;

                                                        </div>
                                                        <div class="custom-control-inline">

                                                            @Html.TextBoxFor(model => model.rejection, new { @class = "form-control r-0 light s-12", @id = "txt_rejection", @oninput = "return CalcRawMaterialTotal();", @placeholder = "%" })
                                                            &nbsp;&nbsp;
                                                            @Html.TextBoxFor(model => model.totalrejection, new { @class = "form-control r-0 light s-12", @onblur = "setDeafult();", @readonly = "readonly", @id = "txt_totalrejection", @placeholder = "Amount" })
                                                        </div>

                                                    </div>
                                                    <div class="form-group col-3 m-0">
                                                        <div>
                                                            <label>Packing,Transport</label> &nbsp;&nbsp;&nbsp;&nbsp;
                                                            <i><label style="font-size:11px">Aprrox Wt(Kg):</label><label id="lbl_appwt" style="font-size:11px"></label></i>

                                                        </div>
                                                        <div class="custom-control-inline">

                                                            @Html.TextBoxFor(model => model.packingtransport, new { @class = "form-control r-0 light s-12", @id = "txt_packingtransport", @oninput = "packing();", @placeholder = "Amount" })
                                                            &nbsp;&nbsp;
                                                            @Html.TextBoxFor(model => model.totalpackingtransport, new { @class = "form-control r-0 light s-12", @onblur = "setDeafult();", @readonly = "readonly", @id = "txt_totalpackingtransport", @placeholder = "Amount" })
                                                        </div>

                                                    </div>
                                                    <div class="form-group col-3 m-0">
                                                        <div>
                                                            <label>Total Other Cost</label> &nbsp;

                                                        </div>
                                                        <div class="custom-control-inline">

                                                            @Html.TextBoxFor(model => model.totallycost, new { @class = "form-control r-0 light s-12", @onblur = "setDeafult();", @readonly = "readonly", @id = "txt_totallycost", @placeholder = "Amount" })
                                                        </div>

                                                    </div>

                                                </div>
                                                <div class="form-group col-4 m-0" style="display:none">
                                                    <label class="Label" for="ChkNewQuotation"></label><br />
                                                    @Html.CheckBox("ChkNewQuotation", true, new { @ID = "chk_NewQuotation" })
                                                    New Quotation

                                                </div>
                                                @*<div class="card my-3 no-b">*@
                                                    @*<div class="form-row col-md-12">
                                                            <div class="form-group col-4 m-0">
                                                                @Html.LabelFor(model => model.SteelCost, new { @class = "Label" })
                                                                @if (ViewData["NatureofWork"].ToString() == "201")
                                                                {
                                                                    @Html.TextBoxFor(model => model.SteelCost, new { @class = "form-control r-0 light s-12", @readonly = "readonly" })
                                                                }
                                                                else
                                                                {
                                                                    @Html.TextBoxFor(model => model.SteelCost, new { @class = "form-control r-0 light s-12", @oninput = "return CalcRawMaterialTotal();" })
                                                                }
                                                            </div>
                                                            <div class="form-group col-4 m-0">
                                                                @Html.LabelFor(model => model.TotalRowMaterialCost, new { @class = "Label" })
                                                                @Html.TextBoxFor(model => model.TotalRowMaterialCost, new { @class = "form-control r-0 light s-12", @readonly = "readonly" })
                                                            </div>
                                                            <div class="form-group col-2 m-0">
                                                                @Html.LabelFor(model => model.Transportation, new { @class = "Label" })
                                                                @Html.TextBoxFor(model => model.Transportation, new { @class = "form-control r-0 light s-12", @oninput = "return CalcRawMaterialTotal();" })
                                                            </div>
                                                            <div class="form-group col-2 m-0">
                                                                @Html.LabelFor(model => model.Total, new { @class = "Label" })
                                                                @Html.TextBoxFor(model => model.Total, new { @class = "form-control r-0 light s-12", @readonly = "readonly" })
                                                            </div>
                                                        </div>*@
