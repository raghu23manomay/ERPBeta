                    <!-- SIGN OUT -->
                    <li class="xn-icon-button pull-right" data-toggle="tooltip" data-placement="bottom" title="Click For Log Out">
                        <a href="@Url.Action("Login", "Auth", new { @Area="" })" class="mb-control" data-box="#mb-signout"><span class="fa fa-sign-out"></span></a>
                    </li>
                    <!-- END SIGN OUT -->
                    <!-- EVENT -->
                    <li class="xn-icon-button pull-right" data-toggle="tooltip" data-placement="bottom" title="Click For Event">
                        <a href="#"><span class="fa fa-globe"></span></a>

                    </li>
                    <!-- END EVENT -->
                    <!-- TASKS -->
