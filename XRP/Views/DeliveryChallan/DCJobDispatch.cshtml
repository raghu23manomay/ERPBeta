                            //console.log(data);
                            $j('#po_qty').val(data.po_qty);
                            $j('#HSNSACCodeM').val(data.HSNCode);
                            $j('#DescriptionM').val(data.Customer_part_no);
                            $j('#HSNCode').val(data.HSNCode);
                            $j('#Customer_part_no').val(data.Customer_part_no);
                        }
                    });
                }
            });


