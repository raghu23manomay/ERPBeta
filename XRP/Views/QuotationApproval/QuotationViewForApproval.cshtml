<script type="text/javascript">

    $j(document).ready(function () {
        if ($j("#hdn_FileStatus").val() == "Conditional Quotation Approved") {
            $j("#dropdownMenuButton").hide();
            $j("#change").show();
            $j("#ToggleDropdown").show();
        }
        else if ($j("#hdn_FileStatus").val() == "Quotation Approved") {
            $j("#dropdownMenuButton").hide();
            $j("#change").hide();
            $j("#ToggleDropdown").hide();
            $j("#download").show();
        }
        else {
            $j("#dropdownMenuButton").show();
            $j("#change").hide();
            $j("#ToggleDropdown").hide();
        }

        //if ($j("#CuttingSquare1").val()) {
        //    $j("#qpdf1_1").show();
        //}
        //if ($j("#Forging1").val()) {
        //    $j("#qpdf1_2").show();
        //}
        //if ($j("#HeatTreatment1").val()) {
        //    $j("#qpdf1_3").show();
        //}
        //if ($j("#ShotBlasting1").val()) {
        //    $j("#qpdf1_4").show();
        //}
        //if ($j("#MPI1").val()) {
        //    $j("#qpdf1_5").show();
        //}
        //if ($j("#Grinding1").val()) {
        //    $j("#qpdf1_6").show();
        //}
        //if ($j("#ColdCoining1").val()) {
        //    $j("#qpdf1_7").show();
        //}
        //if ($j("#Machining1").val()) {
        //    $j("#qpdf1_8").show();
        //}


        var string = $j("#hdn_date").val();
        string = string.split(" ")[0];
        $j("#lbl_cdate").text(string);

        NewUrl = '@Url.Action("QuotationList", "QuotationApproval")';

        $j("#Reject").click(function () {
            //if (confirm('Are you sure you want to Reject this Quotation?')) {

            //} else {

            //}
            // Save it!
            var QuotNo = $j('#QuotationNo').val();
            var CommentFor = $j('#Comment').val();
            var Version = $j('#VersionNo').val();
            var InqNo = $j('#InquiryNo').val();
            $j.ajax({
                url: "../QuotationApproval/RejectQuotation",
                type: "POST",
                dataType: "json",
                data: { QuotationNo: QuotNo, Comment: CommentFor, VersionNo: Version, InquiryNo: InqNo },
                success: function (data) {
                    alert(data);
                    location.href = '@Url.Action("QuotationList", "QuotationApproval")';
                }
            });
        });

        $j("#change").click(function () {

            var InqNo = $j('#InquiryNo').val();

            window.location.href = '@Url.Action("NewQuotation", "Quotation")?id=' + InqNo;
        });


        $j("#ConditionalView").click(function () {

            var InqNo = $j('#InquiryNo').val();

            window.location.href = '@Url.Action("NewQuotation", "Quotation")?id=' + InqNo;
        });
        $j("#Approve").click(function () {
            $('#CommentForApproval').hide();
            //if (confirm('Are you sure you want to Approve this Quotation?')) {

            //} else {

            //}
            // Save it!
            var QuotNo = $j('#QuotationNo').val();
            var CommentFor = $j('#Comment').val();
            var Version = $j('#VersionNo').val();
            var InqNo = $j('#InquiryNo').val();
            $j.ajax({
                url: "../QuotationApproval/ApproveQuotation",
                type: "POST",
                dataType: "json",
                data: { QuotationNo: QuotNo, Comment: CommentFor, VersionNo: Version, InquiryNo: InqNo },
                success: function (data) {
                    alert(data);
                    location.href = '@Url.Action("QuotationList", "QuotationApproval")';
                }
            });
        });


        $j("#ConditionalApprove").click(function () {

            $('#CommentForApproval').hide();
            var QuotNo = $j('#QuotationNo').val();
            var CommentFor = $j('#Comment').val();
            var Version = $j('#VersionNo').val();
            var InqNo = $j('#InquiryNo').val();
            $j.ajax({
                url: "../QuotationApproval/ApproveConditionalQuotation",
                type: "POST",
                dataType: "json",
                data: { QuotationNo: QuotNo, Comment: CommentFor, VersionNo: Version, InquiryNo: InqNo },
                success: function (data) {

                    alert(data);
                    location.href = '@Url.Action("QuotationList", "QuotationApproval")';

                }
