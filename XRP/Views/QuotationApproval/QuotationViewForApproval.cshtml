                                    @*<input type="button" name="Save" value="Download" class="btn btn-info btn-default" onclick="location.href='@Url.Action("QuotationPDF", "Quotation", new { QuotationId = Model.id, _target = "blank" }) '" />*@
                                    @*<input type="button" name="Save" value="Approve" class="btn btn-primary" id="Approve" />
                                    <input type="button" name="Save" value="Reject" class="btn btn-danger btn-default" id="Reject" />*@
