                                    </div>

                                    @*<div class="form-group" style="text-align:center">*@
                                    @*<input type="button" name="Save" value="View All Version" class="btn btn-primary btn-default" id="ViewAllVersion" onclick="location.href = '@Url.Action("GetAllVersionOfOneQuotation", "QuotationApproval", new { QuotationNo = Model.QuotationNo, QuotId=Model.id })'" />*@
                                    @*<a class="editVaccin  btn btn-default btn-rounded" href="/QuotationApproval/QuotationViewForApproval?QuotationId=@data.id" title="View Quotation"><span class="fa fa-edit"></span></a>*@
